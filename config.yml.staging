env: "staging"
sentry_dsn: "$sentry_dsn"
log_level: "info"
redis:
    cache_host: "$staging_redis_host_imaginary_service"
    lock_host: "$staging_redis_lock_host_imaginary_service"
aws:
    region: "$media_region"
    s3_key: "$media_s3_key"
    s3_secret: "$media_s3_secret"
    s3_bucket: "$staging_media_s3_bucket"
cache_ttl: 10000
